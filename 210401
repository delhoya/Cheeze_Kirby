ㅇ SELECT 구문

SELECT column1, column2, …
FROM 테이블 명
WHERE 조건
ORDER BY 정렬 순서;

SELECT 학번,이름 FROM 학생 WHERE 학년 IN (3,4)

SELECT 학과 FROM 학생
SELECT DISTINCT 학과 FROM 학생
SELECT COUNT(DISTINCT 학과) FROM 학생 WHERE 학과 = '컴퓨터학과'

SELECT 학번 FROM 학생 WHERE 이름 LIKE '이%' ORDER BY 학년 DESC


ㅇ 별칭(AS)
https://coding-factory.tistory.com/84

ㅇ 집계함수 
  (COUNT,AVG,MAX,MIN,SUM)   
  https://coding-factory.tistory.com/85?category=772898
  (GROUP BY)
  https://coding-factory.tistory.com/86?category=772898

SELECT 학과 , COUNT(학과) AS '학과별튜플수' FROM 학생 GROUP BY 학과 
SELECT 과목이름, MIN(점수) AS '최소점수' , MAX(점수) AS '최대점수' FROM 성적 GROUP BY 과목 HAVING AVG(점수) >= 90


